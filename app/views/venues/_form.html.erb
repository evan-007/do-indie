<%= form_for @venue do |f| %>
	<%= f.label :name %>
	<%= f.text_field :name %><br />
	<%= f.label :phone %>
	<%= f.text_field :phone %><br />
	<%= f.label :address %>
	<%= f.text_area :address %><br />
	<%= f.label "English bio" %>
	<%= f.text_area :en_bio %><br />
	<%= f.label "Korean bio" %>
	<%= f.text_area :ko_bio %><br />
	<%= f.label :facebook %>
	<%= f.text_field :facebook %><br />
	<%= f.label :cafe %>
	<%= f.text_field :cafe %><br />
	<%= f.label :website %>
	<%= f.text_field :website %><br />
	<%= f.label :photo %>
	<%= f.file_field :avatar %>
	<%= f.submit %>
<% end %>