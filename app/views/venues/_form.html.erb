<%= form_for @venue do |f| %>
	<%= f.label :name %>
	<%= f.text_field :name %><br />
	<%= f.label :phone %>
	<%= f.text_field :phone %><br />
	<%= f.label :address %>
	<%= f.text_area :address %><br />
	<%= f.label :misc %>
	<%= f.text_area :misc %><br />
	<%= f.label :facebook %>
	<%= f.text_field :facebook %><br />
	<%= f.label :cafe %>
	<%= f.text_field :cafe %><br />
	<%= f.label :website %>
	<%= f.text_field :website %><br />
	<%= f.submit %>
<% end %>